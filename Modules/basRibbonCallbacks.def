Sub OnActionButton(control As IRibbonControl)
    ' Callbackname in XML File "onAction"

    ' Callback for event button click
    ' Callback fuer Button Click
    
    Select Case control.ID
        
        Case "btn_database"
            ' In Tab:   tab_setup
            ' In Group: grp_server
